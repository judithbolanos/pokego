{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@styles/*": ["./src/styles/*"],
      "@img/*": ["./src/assets/*"],
      "@components/*": [
        "./src/components/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
